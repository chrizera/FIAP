package br.com.fiap.beans;

public class Pedido {
	private int numero;
	private String data;
	private double total;
	public Pedido(int numero, String data, double total) {
		super();
		this.numero = numero;
		this.data = data;
		this.total = total;
	}
	public Pedido() {
		super();
	}
	public int getNumero() {
		return numero;
	}
	public void setNumero(int numero) {
		this.numero = numero;
	}
	public String getData() {
		return data;
	}
	public void setData(String data) {
		this.data = data;
	}
	public double getTotal() {
		return total;
	}
	public void setTotal(double total) {
		this.total = total;
	}

}
